{
  "manifest_version": 3,
  "name": "Quick Acc",
  "version": "1.0.1",
  "description": "用于多环境多服务的快捷访问",
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "action": {
    "default_title": "Quick Acc",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "tabs"
  ]
}